{
  "name": "blogger",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "populate:db": "npx tsx ./src/utils/mockServer/setupDb.ts",
    "start:backend": "json-server --watch ./src/utils/mockServer/db.json --port 3000",
    "generate:types": "npx openapi-typescript ./public/openapi.yaml -o ./src/types/generated/schema.d.ts",
    "start": "concurrently \"npm run dev\" \"npm run start:backend\""
  },
  "dependencies": {
    "@faker-js/faker": "^9.5.1",
    "axios": "^1.8.1",
    "json-server": "^1.0.0-beta.3",
    "quill": "^2.0.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-toastify": "^0.2.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.2",
    "jsdom": "^26.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vitest": "^3.0.7",
    "vue-tsc": "^2.2.4"
  }
}
